{
  "name": "wiki-frontend",
  "version": "0.1.0",
  "author": "Jessica Bergs",
  "description": "A simple wiki frontend to create and view wiki pages",
  "homepage": "http://localhost:9000",
  "main": "public/index.html",
  "jsnext:main": "src/index.jsx",
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "html-webpack-plugin": "^3.0.7",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-test-renderer": "^16.10.2",
    "webpack-node-externals": "^1.7.2"
  },
  "sasslintConfig": "./.sass-lint.yml",
  "scripts": {
    "build:wp": "webpack --mode production",
    "start:dev": "webpack-dev-server",
    "start": "react-scripts start",
    "build": "npm run prettier && react-scripts build",
    "eject": "react-scripts eject",
    "prettier": "prettier \"./{src,test}/**/*.{js,jsx}\" --write",
    "test": "npm run prettier && npm run test:unit",
    "test:unit": "jest --coverage /*.test.jsx",
    "test:watch": "jest --watch /*.test.jsx",
    "lint": "npm run lint:js && npm run lint:sass",
    "lint:js": "eslint \"./{src,test}/**/*.{js,jsx}\"",
    "lint:sass": "sass-lint -v -q"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "axios": "^0.19.0",
    "babel-cli": "^6.22.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.4",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.0.1",
    "file-loader": "^4.3.0",
    "jest": "^24.9.0",
    "jest-css-modules": "^2.1.0",
    "moment": "^2.24.0",
    "node-sass": "^4.5.2",
    "prettier": "^1.5.3",
    "react-markdown": "^4.2.2",
    "sass-lint": "^1.10.2",
    "sass-loader": "^8.0.0",
    "sass-mq": "^3.3.2",
    "semantic-ui": "^2.4.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.1",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.4.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
